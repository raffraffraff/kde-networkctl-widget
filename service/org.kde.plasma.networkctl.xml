<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN"
 "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.kde.plasma.networkctl">
    <annotation name="org.freedesktop.DBus.Deprecated" value="false"/>
    
    <method name="ListInterfaces">
      <annotation name="org.qtproject.QtDBus.QtTypeName.Out0" value="QVariantList"/>
      <arg name="interfaces" type="aa{sv}" direction="out"/>
      <annotation name="org.freedesktop.DBus.Method.NoReply" value="false"/>
    </method>
    
    <method name="GetStatus">
      <arg name="interface" type="s" direction="in"/>
      <annotation name="org.qtproject.QtDBus.QtTypeName.Out0" value="QVariantMap"/>
      <arg name="status" type="a{sv}" direction="out"/>
    </method>
    
    <method name="SetUp">
      <arg name="interface" type="s" direction="in"/>
      <arg name="success" type="b" direction="out"/>
    </method>
    
    <method name="SetDown">
      <arg name="interface" type="s" direction="in"/>
      <arg name="success" type="b" direction="out"/>
    </method>
    
    <signal name="InterfaceChanged">
      <arg name="interface" type="s"/>
      <arg name="state" type="s"/>
    </signal>
  </interface>
</node>
